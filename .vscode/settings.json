{
  "editor.mouseWheelZoom": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "code-runner.clearPreviousOutput": true,
  "editor.links": false,
  "editor.fontFamily": "'Lilex Nerd Font Mono', 'JetBrains Mono'",
  "editor.fontLigatures": true,
  "window.title": " ",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.fontSize": 16,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": "'Lilex Nerd Font Mono', 'JetBrains Mono'",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "editor.lineNumbers": "off",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "workbench.activityBar.location": "bottom",
  "window.newWindowProfile": "Default",
  "editor.overtypeCursorStyle": "line",
  "terminal.integrated.accessibleViewPreserveCursorPosition": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorStyleInactive": "none",
  "editor.cursorStyle": "underline",
  "workbench.productIconTheme": "fluent-icons",
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "editor.lightbulb.enabled": "off",
  "window.zoomLevel": 1,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "git.openRepositoryInParentFolders": "always",
  "workbench.tree.renderIndentGuides": "always",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.hover.enabled": false,
  "editor.parameterHints.enabled": false,
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "ms-settings"
  ],
  "editor.unicodeHighlight.invisibleCharacters": false,
  "window.commandCenter": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmPasteNative": false,
  "cursor.cpp.disabledLanguages": ["markdown", "scminput"],
  "workbench.layoutControl.enabled": false,
  "workbench.colorTheme": "Vercel (Dark)",
  "files.autoSave": "afterDelay",
  "workbench.iconTheme": "symbols",
  "workbench.panel.showLabels": false,
  "window.menuBarVisibility": "compact"
}
